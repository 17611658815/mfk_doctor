.tooltip {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.6);
  z-index: 999999999;
}

.tooltip_txt {
  width: 80%;
  /* height: 75%; */
  position: absolute;
  top: 45%;
  left: 50%;
  transform: translate(-50%, -50%);
  background: #fff;
  border-radius: 15rpx;
}

.read {
  width: 204rpx;
  font-size: 30rpx;
  height: 72rpx;
  border-radius: 44px 44px 44px 44px;
  background: #6ea7f7;
  color: #fff;
  position: absolute;
  top: 88%;
  left: 50%;
  transform: translate(-50%, -50%);
}

