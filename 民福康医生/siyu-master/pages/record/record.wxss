}

/*录音区域*/

.recordBox {
  width: 100%;
  height: 275rpx;
  position: relative;
}


.recordHead {
  width: 100%;
  height: 275rpx;
}

.content {
  width: 100%;
  margin: auto;
}

.boxs {
  width: 95%;
  min-height: 50rpx;
  margin: auto;
  padding-top: 32rpx;
  padding-bottom: 15rpx;
  font-size: 34rpx;
  color: #333;
  font-weight: 600;
}

