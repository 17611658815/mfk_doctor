  font-size: 34rpx;
  color: #fff;
  background: #ff973b;
  margin: auto;
  justify-content: center;
}

.isPause2 image {
  width: 50rpx;
  height: 70rpx;
  margin: auto;
  margin-left: 37%;
}

.isPause image {
  width: 50rpx;
