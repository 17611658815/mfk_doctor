  color: #999;
}

.fxBox {
  width: 95%;
  height: 80rpx;
  display: flex;
  background: #6ea8f7;
  align-items: center;
  position: fixed;
  left: 50%;
  top: 95%;
  transform: translate(-50%, -50%);
  border-radius: 44px 44px 44px 44px;
}

.fxBox view {
  flex: 1;
  background: none;
  color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  font-weight: 500;
  font-size: 35rpx;
}

.fxBox .btn1 {
  border-right: 1rpx solid #fff;
}

.fxBox .btn1 image {
  width: 40rpx;
  height: 40rpx;
  margin-right: 20rpx;
}

.fxBox .btn2 image {
  width: 40rpx;
  height: 40rpx;
  margin-right: 20rpx;
}

.flowPath {
  padding-top: 56rpx;
  padding-bottom: 40rpx;
  border-bottom: 20rpx solid #f8f8f8;
}

.flowPath>view:nth-child(1) {
  width: 95%;
  margin: auto;
  display: flex;
  justify-content: space-around;
}

.flowPath>view:nth-child(1) .right {
  width: 160rpx;
  height: 5rpx;
  margin-top: 45rpx;
}

.flowPath .flow_2 {
  width: 78rpx;
