/*自定义抽屉样式*/

.action-sheet-item {
  width: 92%;
  margin: 0 auto;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  background: #fff;
  padding: 0;
  font-size: 36rpx;
  color: #333;
}

.action-sheet-cancel.cancelItem {
  width: 92%;
  margin: 0 auto;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  background: #fff;
  padding: 0;
  font-size: 36rpx;
  color: #333;
}

.isPause {
  width: 122rpx;
  height: 122rpx;
  line-height: 122rpx;
  border-radius: 50%;
  display: flex;
  align-content: center;
  padding: 0;
  font-size: 34rpx;
  color: #fff;
  background: #ff973b;
  margin: auto;
  justify-content: center;
}

.isPause2 {
  width: 122rpx;
  height: 122rpx;
  line-height: 122rpx;
  border-radius: 50%;
  display: flex;
